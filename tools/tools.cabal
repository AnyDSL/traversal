description:         Tools to help using the traversal code
license:             LGPL
author:              ArsÃ¨ne PÃ©rard-Gayot
maintainer:          arsene.perard@laposte.net
build-type:          Simple
cabal-version:       >=1.10
 
executable FBufToPng
  main-is:           FBufToPng.hs
  hs-source-dirs:    FBufToPng
  build-depends:     base >=4.5, vector, bytestring, binary, data-binary-ieee754, JuicyPixels
  default-language:  Haskell2010

executable GenPrimary
  main-is:           GenPrimary.hs
  hs-source-dirs:    GenPrimary
  build-depends:     base >=4.5, linear, bytestring, binary, data-binary-ieee754
  default-language:  Haskell2010

executable GenShadow
  main-is:           GenShadow.hs
  hs-source-dirs:    GenShadow
  build-depends:     base >=4.5, linear, vector, bytestring, binary, data-binary-ieee754
  default-language:  Haskell2010

executable GenRandom
  main-is:           GenRandom.hs
  hs-source-dirs:    GenRandom
  build-depends:     base >=4.5, linear, random >= 1.1, bytestring, binary, data-binary-ieee754
  default-language:  Haskell2010

