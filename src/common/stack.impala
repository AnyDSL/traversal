struct Stack {
    data: [i32 * 64],
    id: i32
}

fn empty_stack() -> Stack {
    let undef: i32;
    Stack {
        id: 0,
        data: [undef as i32, ..64]
    }
}

fn push(mut stack: &Stack, i: i32) -> () {
    stack.id++;
    stack.data(stack.id) = i;
}

fn pop(mut stack: &Stack) -> i32 {
    let ret = stack.data(stack.id);
    stack.id--;
    ret
}
